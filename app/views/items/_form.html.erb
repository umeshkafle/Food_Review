<%= form_for :item, url: {action: "create"}, :html => {:multipart => true} do|f|%>
	<p>
		<%= f.label :Title%>
		<%= f.text_field :title%>
	</p>

	<p>
		<%= f.label :Cuisine%>
		<%= f.text_field :Cuisine%>
	</p>

	<p>
		<%= f.label :Description%>
		<%= f.text_area :description%>
	</p>
	<p>
		<%= f.label :Price%>
		<%= f.text_field :price%>
	</p>
	<p>
		<%= f.label :Image%>
		<%= f.file_field :avatar%>
	</p>
	<p><%= submit_tag ("Submit")%></p>
<%end%>